{
  "#": [ "#copy($)" ],
  "flightConsts": {
    "#loop($.flights)": {
      "flightId": "#currentvalueatpath($.flightId)",
      // LANDING
      "landingLength": "#currentvalueatpath($.landingLength)",
      "landingArea": "#multiply(#subtract(#currentvalueatpath($.landingWidth),  2.25), #currentvalueatpath($.landingLength))",
      "supportChannelLength": "#subtract(#subtract(#currentvalueatpath($.landingWidth), 2.25), 4.5)",
      "supportStudQuantity": "(landingWidth - 7.25) / 18, round up",
      "midPanCount": "#subtract(#currentvalueatpath($.risers), 1)",
      // STAIRS
      "topPanArea": "#multiply(#currentvalueatpath($.stairWidth), 10.7)",
      "midpanArea": "#multiply(#currentvalueatpath($.stairWidth), 10.7)",
      "botPanArea": "#multiply(#currentvalueatpath($.stairWidth), 6.25)",
      "stringerArea": "#CostMethods.FlightSolver::StringerArea(#currentvalueatpath($.riseHeight), #currentvalueatpath($.runLength), 11,  #currentvalueatpath($.isDdl), #currentvalueatpath($.riserHeight), 11)",
      // RAILING
      "topRailLength": "#CostMethods.FlightSolver::FindHypotenuse(#currentvalueatpath($.riseHeight), #currentvalueatpath($.runLength), #currentvalueatpath($.isDdl), #currentvalueatpath($.riserHeight), 11)",
      "bottomRailQuantity": "#add(#CostMethods.FlightSolver::IntPostQuantity(#currentvalueatpath($.runLength)), 1)",
      "postHeight": 50,
      "intPostHeight": 49.25,
      "intPostQuantity": "#CostMethods.FlightSolver::IntPostQuantity(#currentvalueatpath($.runLength))",
      "picketLength": 35,
      "picketQuantity": "#CostMethods.FlightSolver::PicketQuantity(#currentvalueatpath($.runLength))",
      // DOG LEG
      "isDogLeg": "#currentvalueatpath($.isDogLeg)",
      "dogLegStringerArea": "#multiply(#currentvalueatpath($.dogLegLength), 11)",
      "dogLegPanArea": "#multiply(#currentvalueatpath($.dogLegLength), #currentvalueatpath($.stairWidth))",
      "dogLegIntPostQuantity": null,
      "dogLegPicketQuantity": null,
      "dogLegTopRailTopRailQuantity": null,
      "dogLegBotRailQuantity": null,
      "dogLegTopRailLength": null,
      "dogLegBottomRailLength": null,
      // DOWNWARD DOG LEG
      "isDdl": "#currentvalueatpath($.isDdl)",
      // "ddlStringerLength: null,
      // "ddlRailLength"
      //"ddlPanArea": (botPan + 11) x stairWidth,
      //"ddlBotPan": 17.25 x stairWidth,
      "ddlStringerArea": "***ADD 1 RISE and RUN***",
      // DOGLEG EXTENSION
      "isDdlExtension": "#currentvalueatpath($.isDdlExtension)"
      //"ddlExtBotPanArea":  ddlExtension x stairWidth
    }
  },
